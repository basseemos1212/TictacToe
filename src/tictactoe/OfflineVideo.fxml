<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="parent" prefHeight="500.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.OfflineVideoController">
    <stylesheets>
        <URL value="@/css/videoplayer.css" />
    </stylesheets>
   <children>
      <VBox alignment="CENTER" layoutX="-1.0" layoutY="-2.0" prefHeight="427.0" prefWidth="800.0">
         <children>
            <Label fx:id="playerName" alignment="CENTER" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <MediaView fx:id="mediaView" fitHeight="350.0" fitWidth="750.0" />
         </children>
      </VBox>
      <Button fx:id="backButton" alignment="CENTER" layoutX="170.0" layoutY="446.0" mnemonicParsing="false" onAction="#onBackClick" text="Back" AnchorPane.leftAnchor="100.0" />
      <Button fx:id="playAgainButton" alignment="CENTER" ellipsisString="" layoutX="602.0" layoutY="446.0" mnemonicParsing="false" onAction="#onPlayAgainClick" text="PlayAgain" textAlignment="CENTER" AnchorPane.rightAnchor="100.0" />
   </children>
</AnchorPane>
