<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>


<DialogPane fx:id="parent" prefHeight="169.0" prefWidth="450.0" stylesheets="@../css/requestmessage.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.RequestMessageController">
   <header>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="450.0">
         <children>
            <Label text="Player">
               <HBox.margin>
                  <Insets left="20.0" right="7.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="playerName" text="PlayerName">
               <HBox.margin>
                  <Insets right="7.0" />
               </HBox.margin>
            </Label>
            <Label alignment="CENTER_RIGHT" text="request to play with you.">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </header>
   <content>
      <HBox fx:id="bottom" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="acceptBtn" ellipsisString="" mnemonicParsing="false" onAction="#onAcceptClick" prefHeight="30.0" prefWidth="75.0" text="Accept" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="10.0" right="200.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="rejectBtn" alignment="CENTER_RIGHT" contentDisplay="RIGHT" ellipsisString="" mnemonicParsing="false" onAction="#onRejectClick" prefHeight="30.0" prefWidth="75.0" text="Reject" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="50.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </content>
</DialogPane>
